{
  "name": "semantrix",
  "version": "1.0.0",
  "type": "module",
  "description": "Lightweight DOM runtime for authoring dynamic HTML in VS Code.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "node -e \"fs.rmSync('dist',{recursive:true,force:true})\"",
    "build": "npm run clean && tsc --project tsconfig.json",
    "prepare": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "examples:serve": "npm run build && npx http-server examples/browser -p 3000"
  },
  "keywords": [
    "dom",
    "jsx",
    "framework",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jsdom": "^21.1.3",
    "@types/node": "^24.5.2",
    "jsdom": "^24.0.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "vitest": "^1.6.0"
  }
}
